<div class="container bg-light w-50 pb-3">
    <h2>Add products</h2>
    <form action="/" method="post" enctype="multipart/form-data" id="form"> 
        <input type="text" name="title" placeholder="Title" class="form-control" id="title">
        <input type="number" name="price" placeholder="Price" class="form-control" id="price">
        <input type="text" name="thumbnail" placeholder="Internet url" class="form-control" id="thumbnail">
        <button class="btn btn-secondary" id="button">Add</button>
    </form>
</div> 